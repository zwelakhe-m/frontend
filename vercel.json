{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "builds": [
    {
      "src": "dist/frontend/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/frontend/server/server.mjs"
    }
  ]
  
}